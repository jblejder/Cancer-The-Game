<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="@android:color/black"/>
            <corners android:radius="2dp"/>
        </shape>
    </item>
    <item
        android:bottom="0.8dp"
        android:left="0.8dp"
        android:right="0.8dp"
        android:top="0.8dp">
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="@color/keytenLight"/>
                    <corners android:radius="2dp"/>
                </shape>
            </item>
            <item>
                <shape android:shape="rectangle">
                    <corners android:radius="2dp"/>
                    <gradient
                        android:angle="90"
                        android:endColor="#1e000000"
                        android:startColor="#a5c6c6c6"/>
                </shape>
            </item>
        </layer-list>
    </item>
    <item>
        <inset
            android:drawable="@drawable/key"
            android:inset="5dp"/>
    </item>
</layer-list>